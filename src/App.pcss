.app {
    text-align: center;
}

.logo {
    animation: logo-spin infinite 20s linear;
    height: 80px;
}

.header {
    height: 150px;
    color: white;
    background-color: #222;
    padding: 20px;
}

.intro {
    font-size: large;
}

@keyframes logo-spin {
    from {
        transform: rotate(0deg);
    }
    to {
        transform: rotate(360deg);
    }
}
